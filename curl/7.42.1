set hasinfo	0

setStandardPaths $name $versiondir $::env(GoochBiotoolRoot) $hasbin $hasinclude $haslib $hasman $hasinfo $haslib64

prepend-path PKG_CONFIG_PATH $::env(GoochBiotoolRoot)/$name/$versiondir/lib/pkgconfig

