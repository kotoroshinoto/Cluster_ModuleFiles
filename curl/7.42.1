#%Module1.0

proc ModulesHelp { } {
        puts stderr "\t\n"
}

source "$::env(GoochBiotoolRoot)/modulefiles/util.tcl"

set name	curl

set versiondir	7.42.1

set hasbin	1
set hasinclude	1
set haslib	1
